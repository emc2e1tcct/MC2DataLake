{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "MC2Factory"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/AzureBlobStorageForSnowflake')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobStorage",
				"typeProperties": {
					"sasUri": "https://mc2datalake.blob.core.windows.net/mc2-bs-dev-golden?sp=racwdlmeop&st=2021-10-22T12:26:16Z&se=2021-10-27T20:26:16Z&skoid=11d43cf5-e65c-492f-9b3c-3592215acde9&sktid=9b03d892-fe8c-4e05-80f1-32e928a16ba2&skt=2021-10-22T12:26:16Z&ske=2021-10-27T20:26:16Z&sks=b&skv=2020-08-04&sv=2020-08-04&sr=c&sig=GrL%2B93XIV55anYYPapxrUxXB4JCSFd8rgi%2B2srkGNeM%3D",
					"sasToken": {
						"type": "SecureString",
						"value": "**********"
					}
				},
				"connectVia": {
					"referenceName": "integrationRuntimeVirtual",
					"type": "IntegrationRuntimeReference"
				}
			},
			"dependsOn": [
				"[variables('factoryId')]"
			]
		}
	]
}