{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "MC2Factory"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/AzureBlobStorageForSnowflake')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobStorage",
				"typeProperties": {
					"sasUri": "https://mc2datalake.blob.core.windows.net/mc2-bs-dev-golden/21102021-mc2_maestro_cebe.parquet?sp=racwdymeop&st=2021-10-22T12:41:03Z&se=2021-10-22T20:41:03Z&spr=https&sv=2020-08-04&sr=b&sig=4oFHxHWRH453LG%2FpUpjhG9a2dInqjOYjp9nVawimqD0%3D",
					"sasToken": {
						"type": "SecureString",
						"value": "**********"
					}
				},
				"connectVia": {
					"referenceName": "integrationRuntimeVirtual",
					"type": "IntegrationRuntimeReference"
				}
			},
			"dependsOn": [
				"[variables('factoryId')]"
			]
		}
	]
}