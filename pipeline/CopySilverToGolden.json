{
	"name": "CopySilverToGolden",
	"properties": {
		"activities": [
			{
				"name": "Copy_asy",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "mc2-bs-dev-silver/[object Object]/"
					},
					{
						"name": "Destination",
						"value": "mc2-bs-dev-golden/[object Object]/"
					}
				],
				"typeProperties": {
					"source": {
						"type": "ParquetSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"wildcardFileName": "*",
							"enablePartitionDiscovery": false
						}
					},
					"sink": {
						"type": "ParquetSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "ParquetWriteSettings"
						}
					},
					"enableStaging": false,
					"validateDataConsistency": false
				},
				"inputs": [
					{
						"referenceName": "SourceSilver",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DestinationGolden",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}