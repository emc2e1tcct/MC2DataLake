{
	"name": "CopyMaestrasMC2ToBronce",
	"properties": {
		"description": "Copia las tablas maestras del dataverse a la capa Bronce",
		"activities": [
			{
				"name": "ForEach_Maestras",
				"type": "ForEach",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@pipeline().parameters.cw_items",
						"type": "Expression"
					},
					"activities": [
						{
							"name": "Copy_Maestras",
							"type": "Copy",
							"dependsOn": [],
							"policy": {
								"timeout": "7.00:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [
								{
									"name": "Source",
									"value": "@{item().source.entityName}"
								},
								{
									"name": "Destination",
									"value": "mc2-bs-dev-bronze//@{item().destination.fileName}"
								}
							],
							"typeProperties": {
								"source": {
									"type": "CommonDataServiceForAppsSource"
								},
								"sink": {
									"type": "DelimitedTextSink",
									"storeSettings": {
										"type": "AzureBlobFSWriteSettings"
									},
									"formatSettings": {
										"type": "DelimitedTextWriteSettings",
										"quoteAllText": true,
										"fileExtension": ".txt"
									}
								},
								"enableStaging": false,
								"validateDataConsistency": false
							},
							"inputs": [
								{
									"referenceName": "SourceDataset_MaestrasDataverse",
									"type": "DatasetReference",
									"parameters": {
										"cw_entityName": "@item().source.entityName"
									}
								}
							],
							"outputs": [
								{
									"referenceName": "DestinationDataset_MaestrasBronce",
									"type": "DatasetReference",
									"parameters": {
										"cw_fileName": "@item().destination.fileName"
									}
								}
							]
						}
					]
				}
			},
			{
				"name": "CopyPrincipalesMC2ToBronce",
				"type": "ExecutePipeline",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"pipeline": {
						"referenceName": "CopyPrincipalesMC2ToBronce",
						"type": "PipelineReference"
					},
					"waitOnCompletion": true
				}
			}
		],
