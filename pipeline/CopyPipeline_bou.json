{
	"name": "CopyPipeline_bou",
	"properties": {
		"activities": [
			{
				"name": "ForEach_bou",
				"description": "Bucle que copia tablas de un origen",
				"type": "ForEach",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@pipeline().parameters.cw_items",
						"type": "Expression"
					},
					"isSequential": true,
					"activities": [
						{
							"name": "Copy_bou",
							"description": "Actividad de la copia  de tablas",
							"type": "Copy",
							"dependsOn": [],
							"policy": {
								"timeout": "7.00:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [
								{
									"name": "Source",
									"value": "@{item().source.entityName}"
								},
								{
									"name": "Destination",
									"value": "mc2-bs-dev-bronze/Pruebas/@{item().destination.fileName}"
								}
							],
							"typeProperties": {
								"source": {
									"type": "CommonDataServiceForAppsSource"
								},
								"sink": {
									"type": "DelimitedTextSink",
									"storeSettings": {
										"type": "AzureBlobFSWriteSettings"
									},
									"formatSettings": {
										"type": "DelimitedTextWriteSettings",
										"quoteAllText": true,
										"fileExtension": ".txt"
									}
								},
								"enableStaging": false,
								"validateDataConsistency": false
							},
							"inputs": [
								{
									"referenceName": "SourceDataset_bou",
									"type": "DatasetReference",
									"parameters": {
										"cw_entityName": "@item().source.entityName"
									}
								}
							],
							"outputs": [
								{
									"referenceName": "DestinationDataset_bou",
									"type": "DatasetReference",
									"parameters": {
										"cw_fileName": "@item().destination.fileName"
									}
								}
							]
						}
					]
				}
			}
		],
		"parameters": {
			"cw_items": {
				"type": "array"
			}
		},
		"folder": {
			"name": "Source/Copy"
		},
		"annotations": []
	}
}