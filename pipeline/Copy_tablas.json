{
	"name": "Copy_tablas",
	"properties": {
		"description": "copia las tablas maestras",
		"activities": [
			{
				"name": "ForEach_fp6",
				"type": "ForEach",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@activity('Copy_fp6').output ",
						"type": "Expression"
					},
					"isSequential": true,
					"activities": [
						{
							"name": "Copy_fp6",
							"type": "Copy",
							"dependsOn": [],
							"policy": {
								"timeout": "7.00:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [
								{
									"name": "Source",
									"value": "@{item().source.entityName}"
								},
								{
									"name": "Destination",
									"value": "mc2-bs-dev-bronze/Pruebas/@{item().destination.fileName}"
								}
							],
							"typeProperties": {
								"source": {
									"type": "CommonDataServiceForAppsSource"
								},
								"sink": {
									"type": "DelimitedTextSink",
									"storeSettings": {
										"type": "AzureBlobFSWriteSettings"
									},
									"formatSettings": {
										"type": "DelimitedTextWriteSettings",
										"quoteAllText": true,
										"fileExtension": ".txt"
									}
								},
								"enableStaging": false,
								"validateDataConsistency": false
							},
							"inputs": [
								{
									"referenceName": "SourceDataset_fp6",
									"type": "DatasetReference",
									"parameters": {
										"cw_entityName": "@item().source.entityName"
									}
								}
							],
							"outputs": [
								{
									"referenceName": "DestinationDataset_fp6",
									"type": "DatasetReference",
									"parameters": {
										"cw_fileName": "@item().destination.fileName"
									}
								}
							]
						}
					]
				}
			}
		],
		"parameters": {
