{
	"name": "DataversePipeline",
	"properties": {
		"description": "Copia de fichero de Dataverse",
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "mc2_tabla"
					},
					{
						"name": "Destination",
						"value": "mc2-bs-dev-bronze/Pruebas/"
					}
				],
				"typeProperties": {
					"source": {
						"type": "CommonDataServiceForAppsSource"
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "DataverseEntity1",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "CopiaTablaDataverse",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}