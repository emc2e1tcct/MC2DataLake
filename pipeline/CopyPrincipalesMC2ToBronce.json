{
	"name": "CopyPrincipalesMC2ToBronce",
	"properties": {
		"description": "Copia las 6 tablas principales del dataverse a la capa bronce",
		"activities": [
			{
				"name": "ForEach_Principales",
				"type": "ForEach",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@pipeline().parameters.cw_itemsPrincipales",
						"type": "Expression"
					},
					"activities": [
						{
							"name": "Copy_principales",
							"type": "Copy",
							"dependsOn": [],
							"policy": {
								"timeout": "7.00:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [
								{
									"name": "Source",
									"value": "@{item().source.entityName}"
								},
								{
									"name": "Destination",
									"value": " mc2-bs-dev-bronze/@{formatDatetime(utcnow(),'yyy')}/@{formatDatetime(utcnow(),'MM')}/@{item().destination.fileName}"
								}
							],
							"typeProperties": {
								"source": {
									"type": "CommonDataServiceForAppsSource"
								},
								"sink": {
									"type": "CommonDataServiceForAppsSink",
									"writeBatchSize": 10,
									"writeBehavior": "upsert"
								},
								"enableStaging": false
							},
							"inputs": [
								{
									"referenceName": "SourceDataset_PrincipalesDataverse",
									"type": "DatasetReference",
									"parameters": {
										"cw_entityName": "@item().source.entityName"
									}
								}
							],
							"outputs": [
								{
									"referenceName": "SourceDataset_PrincipalesDataverse",
									"type": "DatasetReference"
								}
							]
						}
					]
				}
			}
		],
		"parameters": {
			"cw_itemsPrincipales": {
				"type": "array",
				"defaultValue": [
					{
						"source": {
							"entityName": "mc2_comentarios_proyecto"
						},
						"destination": {
							"fileName": "mc2_comentarios_proyecto.txt"
						}
					},
					{
						"source": {
							"entityName": "mc2_elemento_pep"
						},
						"destination": {
							"fileName": "mc2_elemento_pep.txt"
						}
					},
					{
						"source": {
							"entityName": "mc2_lineas_adquira"
						},
						"destination": {
							"fileName": "mc2_lineas_adquira.txt"
						}
					},
					{
						"source": {
							"entityName": "mc2_pedido"
						},
						"destination": {
							"fileName": "mc2_pedido.txt"
						}
					},
					{
						"source": {
							"entityName": "mc2_planificacion"
						},
						"destination": {
							"fileName": "mc2_planificacion.txt"
						}
					},
					{
						"source": {
							"entityName": "mc2_proyecto"
						},
						"destination": {
							"fileName": "mc2_proyecto.txt"
						}
					}
				]
			}
		},
		"folder": {
			"name": "Sources_old/CopyFromMC2ToDataLakeBronce"
		},
		"annotations": []
	}
}